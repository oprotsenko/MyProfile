<?xml version="1.0" encoding="utf-8"?><FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="match_parent">    <androidx.constraintlayout.widget.ConstraintLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        tools:context=".ui.main.viewPager.authorization.profile.contacts.ContactsListActivity">        <androidx.constraintlayout.widget.ConstraintLayout            android:id="@+id/layoutContactsContactsOptions"            style="@style/ContactsListOptionsLayoutMarginStyle"            android:layout_width="match_parent"            android:layout_height="wrap_content"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintHorizontal_bias="1.0"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent">            <androidx.appcompat.widget.AppCompatImageButton                android:id="@+id/buttonContactsBack"                style="@style/ImageButtonBackStyle"                android:layout_width="wrap_content"                android:layout_height="@dimen/zero"                app:layout_constraintBottom_toBottomOf="@+id/textViewContactsContacts"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toTopOf="@+id/textViewContactsContacts" />            <androidx.appcompat.widget.AppCompatTextView                android:id="@+id/textViewContactsContacts"                style="@style/ContactsTitleStyle"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                app:layout_constraintBottom_toBottomOf="@+id/buttonContactsSearch"                app:layout_constraintEnd_toStartOf="@+id/buttonContactsSearch"                app:layout_constraintStart_toEndOf="@+id/buttonContactsBack"                app:layout_constraintTop_toTopOf="@+id/buttonContactsSearch" />            <androidx.appcompat.widget.AppCompatImageButton                android:id="@+id/buttonContactsSearch"                style="@style/ImageButtonSearchStyle"                android:layout_width="wrap_content"                android:layout_height="@dimen/zero"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintTop_toTopOf="parent" />            <androidx.appcompat.widget.AppCompatTextView                android:id="@+id/textViewContactsAddContact"                style="@style/AddContactButtonStyle"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toBottomOf="@+id/textViewContactsContacts" />        </androidx.constraintlayout.widget.ConstraintLayout>        <com.google.android.material.floatingactionbutton.FloatingActionButton            android:id="@+id/floatingButtonContactsUp"            style="@style/FloatingButtonUp"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            app:layout_anchor="@id/recyclerViewContacts"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:srcCompat="@drawable/ic_up_arrow" />        <com.google.android.material.floatingactionbutton.FloatingActionButton            android:id="@+id/floatingButtonContactsDelete"            style="@style/FloatingButtonUp"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:contentDescription="@string/contacts_delete_description"            android:visibility="gone"            app:layout_anchor="@id/recyclerViewContacts"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:srcCompat="@drawable/ic_delete_bucket" />        <androidx.recyclerview.widget.RecyclerView            android:id="@+id/recyclerViewContacts"            style="@style/RecyclerViewStyle"            android:layout_width="match_parent"            android:layout_height="@dimen/zero"            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/layoutContactsContactsOptions"            tools:listitem="@layout/item_contact" />    </androidx.constraintlayout.widget.ConstraintLayout></FrameLayout>